{
  "name": "modern-webpack-starter-project",
  "version": "0.0.1",
  "description": "A simple starter I can use to quickly build on a modern foundation",
  "main": "index.js",
  "repository": "https://github.com/cfv1984/modern-webpack-starter-project",
  "author": "Carlos Vergara <cfvergara@gmail.com>",
  "license": "Apache-2.0",
  "private": false,
  "dependencies": {
    "@babel/core": "7.0.0",
    "@babel/preset-env": "7.0.0",
    "babel-loader": "8.0.0",
    "css-loader": "1.0.0",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "node-sass": "4.8.3",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.0",
    "to-string-loader": "1.1.5",
    "webpack": "4.17.1",
    "webpack-cli": "3.1.0",
    "webpack-dev-server": "3.1.7"
  },
  "scripts": {
    "build": "webpack --config config/webpack/prod.js --mode=production",
    "dev:build": "webpack --config config/webpack/dev.js --mode=development",
    "dev:serve": "webpack-dev-server --config config/webpack/dev.js --mode=development"
  }
