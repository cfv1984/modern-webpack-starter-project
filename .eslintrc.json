{
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "quotes": [
            "error",
            "double",
            {
                "allowTemplateLiterals":true
            }
        ],
        "eol-last": ["error","always"]
    }
}